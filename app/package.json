{
		"name": "wheretopark-app",
		"version": "0.1.0",
		"type": "module",
		"private": true,
		"scripts": {
				"dev": "vite dev",
				"build": "vite build",
				"preview": "vite preview",
				"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
				"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
				"test": "vitest",
				"lint": "prettier --plugin-search-dir . --check . && eslint .",
				"format": "prettier --plugin-search-dir . --write .",
				"ios:build": "CAPACITOR=true vite build && cap sync",
				"ios:run": "CAPACITOR=true vite build && cap sync && cap run ios",
				"ios:dev": "LIVE_RELOAD=true cap copy && vite dev --host",
				"android:build": "CAPACITOR=true vite build && cap sync",
				"android:run": "CAPACITOR=true vite build && cap sync && cap run android",
				"android:dev": "LIVE_RELOAD=true cap copy && vite dev --host"
		},
		"devDependencies": {
				"@capacitor/android": "^5.3.0",
				"@capacitor/app": "^5.0.6",
				"@capacitor/cli": "^5.3.0",
				"@capacitor/core": "^5.3.0",
				"@capacitor/geolocation": "^5.0.6",
				"@capacitor/ios": "^5.3.0",
				"@fontsource-variable/inter": "^5.0.15",
				"@iconify/svelte": "^3.1.4",
				"@mapbox/search-js-core": "1.0.0-beta.17",
				"@sveltejs/adapter-cloudflare": "^2.3.3",
				"@sveltejs/adapter-static": "^2.0.3",
				"@sveltejs/kit": "1.24.1",
				"@tailwindcss/typography": "^0.5.10",
				"@types/geojson": "^7946.0.10",
				"@types/haversine": "^1.1.5",
				"@types/mapbox-gl": "^2.7.13",
				"@typescript-eslint/eslint-plugin": "^6.7.0",
				"@typescript-eslint/parser": "^6.7.0",
				"autoprefixer": "^10.4.15",
				"daisyui": "^3.7.3",
				"dayjs": "^1.11.9",
				"eslint": "^8.49.0",
				"eslint-config-prettier": "^9.0.0",
				"eslint-plugin-svelte": "^2.33.1",
				"geojson": "^0.5.0",
				"haversine": "^1.1.1",
				"mailto-link": "^2.0.0",
				"mapbox-gl": "^2.15.0",
				"opening_hours": "^3.8.0",
				"postcss": "^8.4.29",
				"prettier": "^3.0.3",
				"prettier-plugin-svelte": "^3.0.3",
				"schema-dts": "^1.1.2",
				"svelte": "^4.2.0",
				"svelte-check": "^3.5.1",
				"svelte-markdown": "^0.4.0",
				"tailwindcss": "^3.3.3",
				"tslib": "^2.6.2",
				"typesafe-i18n": "^5.26.2",
				"typescript": "^5.2.2",
				"vite": "^4.4.9",
				"vitest": "^0.34.4"
		}
}
