{
		"name": "wheretopark-webapp",
		"version": "0.3.0",
		"type": "module",
		"private": true,
		"scripts": {
				"dev": "npm-run-all --parallel vite typesafe-i18n",
				"vite": "vite dev --host 0.0.0.0",
				"build": "vite build",
				"preview": "vite preview",
				"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
				"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
				"lint": "prettier --plugin-search-dir . --check . && eslint .",
				"format": "prettier --plugin-search-dir . --write .",
				"typesafe-i18n": "typesafe-i18n"
		},
		"devDependencies": {
				"@sveltejs/adapter-node": "1.2.0",
				"@sveltejs/kit": "1.7.1",
				"@types/geoip-lite": "^1.4.1",
				"@types/haversine": "^1.1.5",
				"@types/mapbox-gl": "^2.7.10",
				"@types/node": "^18.14.0",
				"@typescript-eslint/eslint-plugin": "^5.52.0",
				"@typescript-eslint/parser": "^5.52.0",
				"eslint": "^8.34.0",
				"eslint-config-prettier": "^8.6.0",
				"eslint-plugin-svelte3": "^4.0.0",
				"npm-run-all": "^4.1.5",
				"prettier": "^2.8.4",
				"prettier-plugin-svelte": "^2.9.0",
				"svelte": "^3.55.1",
				"svelte-check": "^3.0.3",
				"svelte-preprocess": "^5.0.1",
				"tslib": "^2.5.0",
				"typesafe-i18n": "^5.24.1",
				"typescript": "^4.9.5",
				"vite": "^4.1.2"
		},
		"dependencies": {
				"@aws-sdk/client-appconfigdata": "^3.363.0",
				"@svelteuidev/composables": "^0.10.0",
				"@svelteuidev/core": "^0.10.0",
				"@types/geojson": "^7946.0.10",
				"dayjs": "^1.11.7",
				"geoip-lite": "^1.4.7",
				"geojson": "^0.5.0",
				"haversine": "^1.1.1",
				"mapbox-gl": "^2.12.1",
				"opening_hours": "^3.8.0",
				"surrealdb.js": "^0.5.0",
				"svelte-markdown": "^0.2.3"
		}
