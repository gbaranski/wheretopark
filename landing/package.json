{
	"name": "wheretopark-website",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "npm-run-all --parallel vite typesafe-i18n",
		"vite": "vite",
		"typesafe-i18n": "typesafe-i18n",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"deploy:cloudflare": "vite build && wrangler pages deploy --branch production --project-name wheretopark-landing build",
		"devploy:cloudflare": "vite build && wrangler pages deploy --branch preview --project-name wheretopark-landing build"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.20.4",
		"@tailwindcss/typography": "^0.5.10",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.15",
		"daisyui": "^3.7.3",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"mailto-link": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.29",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"schema-dts": "^1.1.2",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-inview": "^4.0.1",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.4.1",
		"typesafe-i18n": "^5.26.2",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module"
}
